<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Copy">
        <Item>
          <IdeaActionID>$Copy</IdeaActionID>
          <ShortCut>'Ctrl+C' or 'Ctrl+Insert'</ShortCut>
          <Description>Copy</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'Ctrl+B' or 'Ctrl+Button1 Click' or 'Button2 Click'</ShortCut>
          <Description>Declaration</Description>
          <Count>6</Count>
        </Item>
      </Statistic>
      <Statistic Action="Hide All Windows">
        <Item>
          <IdeaActionID>HideAllWindows</IdeaActionID>
          <ShortCut>'Ctrl+Shift+F12'</ShortCut>
          <Description>Hide All Windows</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Move...">
        <Item>
          <IdeaActionID>Move</IdeaActionID>
          <ShortCut>'F6'</ShortCut>
          <Description>Move...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Restore Windows">
        <Item>
          <IdeaActionID>HideAllWindows</IdeaActionID>
          <ShortCut>'Ctrl+Shift+F12'</ShortCut>
          <Description>Restore Windows</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'RecordmeApplication'">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'Shift+F10'</ShortCut>
          <Description>Run 'RecordmeApplication'</Description>
          <Count>9</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'Unnamed'">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'Shift+F10'</ShortCut>
          <Description>Run 'Unnamed'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'RecordmeApplication'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'Ctrl+F2'</ShortCut>
          <Description>Stop 'RecordmeApplication'</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'Unnamed'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'Ctrl+F2'</ShortCut>
          <Description>Stop 'Unnamed'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Version Control">
        <Item>
          <IdeaActionID>ActivateVersionControlToolWindow</IdeaActionID>
          <ShortCut>'Alt+9'</ShortCut>
          <Description>Version Control</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>